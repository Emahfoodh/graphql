{
    "compilerOptions": {
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "target": "ES2020",
        "jsx": "react",
        "allowImportingTsExtensions": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "strict": true,
        "checkJs": true
    },
    "include": [
        "src/**/*"
, "src/App.js"    ],
    "exclude": [
        "node_modules",
        "**/node_modules/*"
    ]
}